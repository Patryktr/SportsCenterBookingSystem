						"body": {
							"mode": "raw",
							"raw": "{\n  \"facilityId\": 1,\n  \"customerPublicId\": \"{{customerPublicId}}\",\n  \"start\": \"2026-01-25T10:00:00Z\",\n  \"end\": \"2026-01-25T12:00:00Z\",\n  \"playersCount\": 2,\n  \"type\": 1\n}"
						},
